{
  "name": "play-ui",
  "version": "1.0.4",
  "description": "A Component Library for Vue 3",
  "license": "MIT",
  "author": "jwhupup",
  "files": [
    "dist"
  ],
  "main": "dist/lib/index.js",
  "module": "dist/es/index.mjs",
  "types": "dist/es/index.d.ts",
  "style": "dist/style/index.css",
  "exports": {
    ".": {
      "types": "./dist/es/index.d.ts",
      "import": "./dist/es/index.mjs",
      "require": "./dist/lib/index.js"
    },
    "./dist/es": "./dist/es/index.mjs",
    "./dist/lib": "./dist/lib/index.js",
    "./dist/es/*.mjs": "./dist/es/*.mjs",
    "./dist/es/*": "./dist/es/*.mjs",
    "./dist/lib/*.js": "./dist/lib/*.js",
    "./dist/lib/*": "./dist/lib/*.js",
    "./dist/*": "./dist/*"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": [
    "dist/**/*.css"
  ],
  "scripts": {
    "build": "rimraf dist && vue-tsc && gulp --require @esbuild-kit/cjs-loader -f gulpfile.ts"
  }
}
